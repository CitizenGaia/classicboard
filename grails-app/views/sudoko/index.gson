import dk.lejengnaver.Sudoko

model {
    Iterable<Sudoko> sudokoList
    Integer sudokoCount
    Integer max
    Integer offset
    String sort
    String order
}
json {
    hal.paginate(Sudoko, sudokoCount, offset, max, sort, order)
    games tmpl.sudoko(sudokoList ?: [])
    count sudokoCount
    max max
    offset offset
    sort sort
    order order
}